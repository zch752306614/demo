<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.alice.zhang.demoapi.demo.module.dao.UserMapper">

    <resultMap id="BaseResultMap" type="com.alice.zhang.demoapi.demo.module.entity.User">
        <id column="id" property="id"/>
        <result column="age" property="age"/>
        <result column="info" property="info"/>
        <result column="name" property="name"/>
    </resultMap>

    <insert id="insert" parameterType="com.alice.zhang.demoapi.demo.module.entity.User">
        insert into user(age,info,name) values(#{age},#{info},#{name})
    </insert>

    <insert id="insert_sql">
        ${sql}
    </insert>

</mapper>